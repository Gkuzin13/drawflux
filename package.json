{
  "name": "drawflux",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev:client": "pnpm --filter ./apps/client dev",
    "dev:server": "pnpm --filter ./apps/server dev",
    "test:client": "pnpm --filter ./apps/client test",
    "test:client:watch": "pnpm --filter ./apps/client test:watch",
    "test:server": "pnpm --filter ./apps/server test",
    "test:server:watch": "pnpm --filter ./apps/server test:watch",
    "g:lint": "pnpm -r lint",
    "g:fix-all-files": "pnpm -r fix-all-files",
    "g:test": "pnpm -r test",
    "g:build": "pnpm -r build",
    "g:typecheck": "pnpm -r typecheck",
    "packages:build": "pnpm --filter '{./packages/**}' build",
    "packages:lint": "pnpm --filter '{./packages/**}' lint",
    "packages:typecheck": "pnpm --filter '{./packages/**}' typecheck"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "react": "^18.2.0",
    "shared": "workspace:^",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.31",
    "@vitest/coverage-v8": "^0.34.2",
    "eslint": "^8.38.0",
    "msw": "^2.0.11",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  }
}
